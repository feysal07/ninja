CREATE TABLE `master_data_value` (
  `masterDataId` bigint(20) NOT NULL,
  `mnemonic` varchar(20) NOT NULL,
  `value` varchar(100) NOT NULL,
  `orderId` bigint(20) NOT NULL,
  `isActive` bit(1) DEFAULT NULL,
  PRIMARY KEY (`masterDataId`),
  KEY `FK_master_data_value` (`mnemonic`),
  CONSTRAINT `FK_master_data_value` FOREIGN KEY (`mnemonic`) REFERENCES `master_data_type` (`mnemonic`) ON DELETE NO ACTION ON UPDATE NO ACTION
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

