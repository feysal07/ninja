CREATE TABLE `advance_setting_user_map` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `userId` bigint(20) DEFAULT NULL,
  `advanceSettingId` int(11) DEFAULT NULL,
  `advanceSettingValue` int(1) DEFAULT NULL,
  PRIMARY KEY (`id`),
  KEY `advanceSettingId` (`advanceSettingId`),
  CONSTRAINT `advance_setting_user_map_ibfk_1` FOREIGN KEY (`advanceSettingId`) REFERENCES `advance_setting` (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=11 DEFAULT CHARSET=utf8;

